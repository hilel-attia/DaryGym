 <h2 class="title-decor">Nos exercices<span><br>en ligne</span></h2>



  {% for categorie in categories %}


    <br><button type="button" class="btn btn-info" data-toggle="collapse" data-target="#{{ categorie.libelle }}">{{ categorie.libelle }}</button>

  {% for exercice in exercices %}
{% if categorie.libelle==exercice.Exercicecategorie %}

  <div id="{{ categorie.libelle }}" class="collapse"><br>

    <div class="container">

     <div class="row">
      <div class="col-md-6 out-trainer-col">
       <div class="out-trainer-item">
        <a  class="fancybox-video" ><video controls src="{{ asset('uploads/videos/') ~exercice.video  }}" type="video/mp4" width="192" height="140"></video>
        </a>
        <div class="out-trainer-info">
          <h3><a>{{ exercice.nom }}</a></h3>
         <h6><a  style="color: black" href="{{asset('uploads/documents/') ~exercice.docs }}" download>Ce lien télécharge le fichier PDF de {{ exercice.nom }}</a></h6>
         <div class="row">
          <div class="span4 collapse-group">

           <p> <span class="collapse" id="viewdetails3"> {{ exercice.description }}</span> <a data-toggle="collapse" data-target="#viewdetails3 " style="color: black">More... &raquo;</a></p>
          </div>
         </div>
        </div>



        </div>
       </div>
      </div>
     </div>
    </div>


 </div>

{% endif %}

 {% endfor %}
 </div>
